<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Exactscoreâ„¢</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">
   <link href="style.css" rel="stylesheet" type="text/css"
   

  </head>
  <body>
    <% for (let i = 0; i < value.length; i++) { %>
        <div class="card">
            <div class="card-body">
              <h5 class="card-title"><%= value[i].subject %></h5>
              <p class="card-text">Pass: <%= value[i].pass %></p>
              <p class="card-text">Fail: <%= value[i].fail %></p>
              <p class="card-text">Absent: <%= value[i].absent %></p>
         
              <a href="<%= "/viewStudents?subject=" + encodeURIComponent(value[i].subject)  %>" class="btn btn-primary">View Students</a>
            </div>
          </div>
    <% } %>

      <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js" integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN" crossorigin="anonymous"></script>
  </body>
  
</html>